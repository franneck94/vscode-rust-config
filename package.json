{
    "name": "vscode-rust-config",
    "displayName": "Rust Config",
    "description": "Creates config files for modern Rust projects",
    "version": "0.2.0",
    "publisher": "franneck94",
    "license": "MIT",
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#9c9c9c",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "tags": [
        "Rust",
        "configuration"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/franneck94/vscode-rust-config.git"
    },
    "homepage": "https://github.com/franneck94/vscode-rust-config/README.md",
    "bugs": {
        "url": "https://github.com/franneck94/vscode-rust-config/issues"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/main",
    "contributes": {
        "taskDefinitions": [
            {
                "type": "Rust_Config"
            }
        ],
        "commands": [
            {
                "command": "Rust_Config.generateConfigRust",
                "title": "Generate Rust Config Files",
                "category": "Rust Config"
            }
        ]
    },
    "scripts": {
        "webpack": "webpack --mode development",
        "webpack-watch": "webpack --mode development --watch",
        "vscode:prepublish": "npm run webpack",
        "compile": "npm run webpack",
        "compile-watch": "npm run webpack-watch",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/eslint-plugin-tslint": "^4.20.0",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^32.3.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "prettier": "2.2.1",
        "typescript": "^4.0.2",
        "webpack": "^5.28.0",
        "webpack-cli": "^4.6.0"
    },
    "dependencies": {
        "json5": "^2.2.0",
        "ts-loader": "^8.1.0",
        "tslint": "^6.1.3",
        "vsce": "^1.87.1",
        "vscode-test": "^1.6.1"
    }
}
