{
    "name": "vscode-rust-config",
    "displayName": "Rust Config",
    "description": "Creates config files for modern Rust projects",
    "version": "0.3.0",
    "publisher": "franneck94",
    "license": "MIT",
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#9c9c9c",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "tags": [
        "Rust",
        "configuration"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/franneck94/vscode-rust-config.git"
    },
    "homepage": "https://github.com/franneck94/vscode-rust-config/README.md",
    "bugs": {
        "url": "https://github.com/franneck94/vscode-rust-config/issues"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/main",
    "contributes": {
        "taskDefinitions": [
            {
                "type": "Rust_Config"
            }
        ],
        "commands": [
            {
                "command": "Rust_Config.generateConfigRust",
                "title": "Generate Rust Config Files",
                "category": "Rust Config"
            }
        ]
    },
    "scripts": {
        "webpack": "webpack --mode development",
        "webpack-watch": "webpack --mode development --watch",
        "vscode:prepublish": "npm run webpack",
        "compile": "npm run webpack",
        "compile-watch": "npm run webpack-watch",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/glob": ">=8.1.0",
        "@types/mocha": ">=10.0.1",
        "@types/node": ">=18.15.3",
        "@types/vscode": ">=1.74.0",
        "@typescript-eslint/eslint-plugin": ">=5.55.0",
        "@typescript-eslint/eslint-plugin-tslint": ">=5.55.0",
        "@typescript-eslint/parser": ">=5.55.0",
        "eslint": ">=8.36.0",
        "eslint-plugin-import": ">=2.27.5",
        "eslint-plugin-jsdoc": ">=40.0.3",
        "glob": ">=9.3.0",
        "mocha": ">=10.2.0",
        "prettier": ">=2.8.4",
        "typescript": ">=5.0.2",
        "webpack": ">=5.76.2",
        "webpack-cli": ">=5.0.1"
    },
    "dependencies": {
        "json5": "^2.2.0",
        "ts-loader": "^9.4.2",
        "tslint": "^6.1.3",
        "vsce": "^2.15.0"
    }
}
